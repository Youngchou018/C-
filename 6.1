#include<iostream>
#include<cctype>
int main()
{
	using namespace std;
	char ch;
	ch = cin.get();
	while (ch != '@')
	{
		if (isdigit(ch))
		{
			cin.get(ch);
			continue;
		}
		else if (islower(ch))
		{ 
			ch=toupper(ch);
			cout << ch;
		}
			
		else if (isupper(ch))
		{
			ch=tolower(ch);
			cout << ch;
		}
		else
			cout << ch;
		cin.get(ch);
	}
	return 0;
}
